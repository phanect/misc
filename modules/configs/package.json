{
  "name": "@phanect/configs",
  "version": "2024.8.7",
  "description": "Collection of @phanect's personal configuration files",
  "type": "module",
  "module": "./dist/eslint.js",
  "types": "./dist/eslint.d.ts",
  "exports": {
    "./eslint": {
      "import": {
        "types": "./dist/eslint.d.ts",
        "default": "./dist/eslint.js"
      }
    },
    "./tsconfig/cloudflare": "./static/tsconfig/tsconfig.cloudflare.json",
    "./tsconfig/frontend": "./static/tsconfig/tsconfig.frontend.json",
    "./tsconfig/nodejs": "./static/tsconfig/tsconfig.nodejs.json",
    "./tsconfig/addons/jsx": "./static/tsconfig/addons/tsconfig.jsx.json",
    "./tsconfig/addons/npm": "./static/tsconfig/addons/tsconfig.npm.json",
    "./tsconfig/addons/vite": "./static/tsconfig/addons/tsconfig.vite.json"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "lint": "tsc --noEmit && : module-configs",
    "release": "npm publish . --access public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:phanect/configs.git"
  },
  "author": "Jumpei Ogawa (https://phanective.org)",
  "license": "CC0-1.0",
  "engines": {
    "node": ">=18.x",
    "npm": ">=10.x"
  },
  "dependencies": {
    "@eslint/js": "^9.7.0",
    "@stylistic/eslint-plugin": "^2.6.1",
    "eslint-plugin-jsdoc": "^50.2.2",
    "eslint-plugin-jsonc": "^2.15.1",
    "eslint-plugin-n": "^17.7.0",
    "eslint-plugin-promise": "^7.0.0",
    "eslint-plugin-vitest": "^0.5.4",
    "typescript-eslint": "^8.0.0"
  },
  "devDependencies": {
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.0.0",
    "deepmerge": "^4.3.1",
    "eslint": "^9.7.0",
    "tsup": "^8.2.4",
    "typescript": "^5.3.3",
    "vitest": "^2.0.2"
  },
  "peerDependencies": {
    "eslint": "^9.7.0",
    "next": "*",
    "svelte": "*",
    "typescript": "*"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "svelte": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  }
}
