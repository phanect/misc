{
  "name": "@phanect/lint",
  "version": "2024.8.7",
  "description": "Personal ESLint configuration for Jumpei Ogawa (@phanect)",
  "type": "module",
  "module": "./dist/eslint.js",
  "types": "./dist/eslint.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/eslint.d.ts",
        "default": "./dist/eslint.js"
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "lint": "tsc --noEmit && : lint",
    "release": "npm publish . --access public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:phanect/eslint-config-phanective.git"
  },
  "author": "Jumpei Ogawa (https://phanective.org)",
  "license": "CC0-1.0",
  "engines": {
    "node": ">=18.x",
    "npm": ">=10.x"
  },
  "dependencies": {
    "@eslint/js": "^9.7.0",
    "@stylistic/eslint-plugin": "^2.6.1",
    "eslint-plugin-jsdoc": "^50.2.2",
    "eslint-plugin-jsonc": "^2.15.1",
    "eslint-plugin-n": "^17.7.0",
    "eslint-plugin-promise": "^7.0.0",
    "eslint-plugin-vitest": "^0.5.4",
    "typescript-eslint": "^8.0.0"
  },
  "devDependencies": {
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.0.0",
    "eslint": "^9.7.0",
    "tsup": "^8.2.4",
    "typescript": "^5.3.3",
    "vitest": "^2.0.2"
  },
  "peerDependencies": {
    "eslint": "^9.7.0",
    "next": "*",
    "svelte": "*",
    "typescript": "*"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "svelte": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  }
}
