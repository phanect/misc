{
  "name": "eslint-config-phanective",
  "version": "2024.8.7",
  "description": "Personal ESLint configuration for Jumpei Ogawa (@phanect)",
  "type": "module",
  "module": "./dist/eslint.mjs",
  "types": "./dist/eslint.d.ts",
  "exports": {
    ".": {
      "import": {
        "default": "./dist/eslint.mjs",
        "types": "./dist/eslint.d.ts"
      }
    }
  },
  "scripts": {
    "build": "unbuild",
    "test": "npm run build && vitest run",
    "lint": "tsc --noEmit && npm run build && eslint",
    "fix": "npm run build && eslint --fix",
    "rwt": "esno ./scripts/rwt.ts",
    "clean": "git clean -dX --force",
    "release": "npm run build && npm publish . --access public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:phanect/eslint-config-phanective.git"
  },
  "author": "Jumpei Ogawa (https://phanective.org)",
  "license": "CC0-1.0",
  "engines": {
    "node": ">=18.x",
    "npm": ">=10.x"
  },
  "dependencies": {
    "@eslint/js": "^9.7.0",
    "@nuxt/eslint-config": "^0.5.0",
    "@stylistic/eslint-plugin": "^2.6.1",
    "eslint-plugin-jsdoc": "^50.2.2",
    "eslint-plugin-jsonc": "^2.15.1",
    "eslint-plugin-n": "^17.7.0",
    "eslint-plugin-promise": "^7.0.0",
    "eslint-plugin-svelte": "^2.39.0",
    "eslint-plugin-vitest": "^0.5.4",
    "eslint-plugin-vue": "^9.26.0",
    "typescript-eslint": "^8.0.0"
  },
  "devDependencies": {
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.0.0",
    "deepmerge": "^4.3.1",
    "eslint": "^9.7.0",
    "typescript": "^5.3.3",
    "unbuild": "v3.0.0-rc.7",
    "vitest": "^2.0.2"
  },
  "peerDependencies": {
    "eslint": "^9.7.0",
    "next": "*",
    "svelte": "*",
    "typescript": "*"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "svelte": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "overrides": {
    "@typescript-eslint/utils": "8.0.0"
  }
}
